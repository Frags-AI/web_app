{
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "jsx":"react-jsx",
    "types": ["vite/client"],
    "module": "ESNext",
    "target":"ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": ["./src/*"],
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}